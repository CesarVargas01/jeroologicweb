              <input
                type="email"
                id="email"
                autocomplete="email"
                class="w-full p-3.5 border border-light-gray dark:border-gray-600 rounded-lg text-base transition-colors duration-300 focus:outline-none focus:border-primary dark:bg-dark dark:text-light"
                required
              />
            </div>

            <div class="mb-5">
              <label
                for="company"
                class="block mb-2 font-medium text-dark dark:text-light"
                >Empresa (Opcional)</label
              >
              <input
                type="text"
                id="company"
                name="company"
                autocomplete="organization"
                class="w-full p-3.5 border border-light-gray dark:border-gray-600 rounded-lg text-base transition-colors duration-300 focus:outline-none focus:border-primary dark:bg-dark dark:text-light"
              />
            </div>

            <div class="mb-5">
              <label
                for="message"
                class="block mb-2 font-medium text-dark dark:text-light"
                >Mensaje</label
              >
              <textarea
                id="message"
                autocomplete="off"
                class="w-full p-3.5 border border-light-gray dark:border-gray-600 rounded-lg text-base transition-colors duration-300 focus:outline-none focus:border-primary min-h-[120px] resize-y dark:bg-dark dark:text-light"
                required></textarea>
            </div>

            <button
              type="submit"
              class="inline-block bg-primary text-white px-7 py-3 rounded-full font-semibold transition-all duration-300 border-none cursor-pointer text-base hover:bg-primary-dark hover:-translate-y-1.5 hover:shadow-lg"
              >Enviar Mensaje</button
            >
          </form>
        </div>
      </div>
    </div>
  </section>
</Layout>