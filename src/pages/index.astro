</section>

<!-- Technologies Section -->
  <section class="py-[100px] bg-white dark:bg-dark" id="technologies">
    <div class="w-full max-w-screen-xl mx-auto px-5">
      <div class="text-center mb-[60px]">
        <h2
          class="text-4xl text-dark dark:text-light mb-4 relative inline-block"
        >
          Tecnologías que Utilizamos
          <div
            class="absolute bottom-[-10px] left-1/2 -translate-x-1/2 w-[200px] h-[4px] bg-primary rounded-md"
          >
          </div>
        </h2>
        <p class="text-gray dark:text-light-gray max-w-3xl mt-5 mx-auto">
          Algunas herramientas y soluciones tecnológicas para soluciones
          escalables y eficientes.
        </p>
      </div>
      <div
        class="grid grid-cols-3 sm:grid-cols-4 md:grid-cols-5 lg:grid-cols-7 gap-8 justify-items-center mt-[50px]"
      >
        {technologies.map(tech => (
          <TechnologyLogo src={tech.src} alt={tech.alt} />
