---
import Layout from '../layouts/Layout.astro';
import { Icon } from 'astro-icon/components';
import ServiceCard from '../components/ServiceCard.astro';
import TechnologyLogo from '../components/TechnologyLogo.astro';
import AdvantageCard from '../components/AdvantageCard.astro';
import Footer from '../components/Footer.astro';

const services = [
  { icon: 'mdi:robot', title: 'Automatizaciones', description: 'Automatizamos tus procesos con herramientas como n8n y Make, reduciendo tiempos y errores.' },
  { icon: 'mdi:database', title: 'Bases de Datos', description: 'Diseñamos e implementamos bases de datos eficientes para gestionar tu información crítica.' },
  { icon: 'mdi:cloud', title: 'Soluciones Cloud', description: 'Implementamos aplicaciones escalables en la nube, en tu hosting o Vps.' },
  { icon: 'mdi:laptop', title: 'Desarrollo Web', description: 'Creación de sitios web y aplicaciones a medida con tecnologías modernas y flexibles.' },
  { icon: 'mdi:sync', title: 'Integraciones', description: 'Conectamos tus sistemas y aplicaciones para un flujo de trabajo unificado y eficiente.' },
  { icon: 'mdi:chart-line-variant', title: 'Análisis de Datos', description: 'Transformamos tus datos en información valiosa para la toma de decisiones estratégicas.' },
];

const technologies = [
  { src: "/images/logos/N8n.svg", alt: "N8n" },
  { src: "/images/logos/make.svg", alt: "Make" },
  { src: "/images/logos/zapier.svg", alt: "Zapier" },
  { src: "/images/logos/mysql.svg", alt: "MySQL" },
  { src: "/images/logos/postgresql.svg", alt: "PostgreSQL" },
  { src: "/images/logos/python.svg", alt: "Python" },
  { src: "/images/logos/nodejs.svg", alt: "Node.js" },
  { src: "/images/logos/typescript.svg", alt: "TypeScript" },
  { src: "/images/logos/reactjs.svg", alt: "React" },
  { src: "/images/logos/angular.svg", alt: "Angular" },
  { src: "/images/logos/hubspot.svg", alt: "HubSpot" },
  { src: "/images/logos/google_analytics.svg", alt: "Google Analytics" },
  { src: "/images/logos/powerbi.svg", alt: "Power BI" },
  { src: "/images/logos/docker.svg", alt: "Docker" },
  { src: "/images/logos/firebase.svg", alt: "Firebase" },
  { src: "/images/logos/mongodb.svg", alt: "MongoDB" },
  { src: "/images/logos/amazon_aws.svg", alt: "Amazon AWS" },
  { src: "/images/logos/google_cloud.svg", alt: "Google Cloud" },
  { src: "/images/logos/chatgpt.svg", alt: "ChatGPT" },
  { src: "/images/logos/gemini.svg", alt: "Gemini" },
  { src: "/images/logos/linux.svg", alt: "Linux" },
