const advantages = [
  { icon: 'mdi:wallet', title: 'Reducción de Costos', description: 'Utilizamos herramientas para ofrecer soluciones de bajo costo sin sacrificar calidad.' },
  { icon: 'mdi:arrow-expand', title: 'Escalabilidad', description: 'Nuestras soluciones están diseñadas para crecer con tu negocio, adaptándose a tus necesidades cambiantes.' },
  { icon: 'mdi:lock-open-outline', title: 'Tecnologías Flexibles', description: 'Trabajamos con tecnologías flexibles que ofrecen transparencia, seguridad y libertad de modificación.' },
  { icon: 'mdi:rocket', title: 'Implementación Rápida', description: 'Gracias a nuestras herramientas y experiencia, implementamos soluciones en tiempo récord.' },
  { icon: 'mdi:cog-outline', title: 'Personalización Total', description: 'Desarrollamos soluciones a medida que se adaptan perfectamente a tus procesos específicos.' },
  { icon: 'mdi:headphones', title: 'Soporte Continuo', description: 'Ofrecemos soporte técnico y mantenimiento continuo para garantizar el funcionamiento óptimo.' },
];
---

<Layout
  title="Soluciones de Software y Automatización | Desarrollo Web y Bases de Datos | Jeroo Logic"
  description="Jeroo Logic ofrece soluciones de software a medida: automatización de procesos con n8n y Make, desarrollo web, bases de datos y análisis de datos. Optimiza tu negocio con tecnología eficiente y escalable."
  image="/images/SolucionesdeSoftware-large.webp"
>

<!-- Hero Section -->
<section
  class="pt-24 pb-20 lg:pt-32 lg:pb-32 bg-gradient-to-br from-sky-50 to-sky-100 dark:from-sky-900 dark:to-sky-800 relative overflow-hidden"
  id="home"
>
  <div class="w-full max-w-screen-xl mx-auto px-5">
    <div class="flex lg:flex-row flex-col items-center justify-between">
      <div class="flex-1 max-w-2xl lg:text-left text-center">
        <h1
          class="text-5xl font-bold leading-tight mb-5 text-dark dark:text-light"
        >
          Automatización de Procesos y Desarrollo Web con Jeroo Logic
        </h1>
        <p class="text-lg text-gray dark:text-light-gray mb-8">
          Jeroo Logic desarrolla herramientas y automatizaciones escalables
          utilizando soluciones tecnológicas. Optimizamos tus procesos con
          soluciones eficientes y de bajo costo.
        </p>
        <div class="flex gap-4 justify-center lg:justify-start">
          <a
            href="#services"
            class="inline-block bg-primary text-white px-7 py-3 rounded-full font-semibold transition-all duration-300 border-none cursor-pointer text-base hover:bg-primary-dark hover:-translate-y-1.5 hover:shadow-lg dark:bg-primary-light dark:border-2 dark:border-primary-light dark:text-white dark:hover:bg-primary-light dark:hover:text-white"
            >Nuestros Servicios</a
          >
          <a
            href="#contact"
            class="inline-block bg-transparent border-2 border-primary text-primary px-7 py-3 ml-4 rounded-full font-semibold transition-all duration-300 cursor-pointer text-base hover:bg-primary hover:text-white hover:-translate-y-1.5 hover:shadow-lg dark:border-primary-light dark:text-primary-light dark:hover:bg-primary-light dark:hover:text-white"
            >Contáctanos</a
          >
        </div>
      </div>
      <div class="flex-1 flex justify-center animate-float mt-12 lg:mt-0">
        <picture
          ><source
            srcset="/images/SolucionesdeSoftware-large.webp 1200w, /images/SolucionesdeSoftware-medium.webp 800w, /images/SolucionesdeSoftware-small.webp 400w"
            sizes="(max-width: 768px) 100vw, 800px"
            type="image/webp"
          />
          <img
            src="/images/SolucionesdeSoftware-large.webp"
            alt="Desarrollo de Software"
            width="800"
            height="450"
            class="max-w-full h-auto rounded-lg shadow-2xl"
            fetchpriority="high"
          />
        </picture>
      </div>
    </div>
  </div>
</section>

<!-- Services Section -->
<section class="py-[100px] bg-white dark:bg-dark" id="services">
  <div class="w-full max-w-screen-xl mx-auto px-5">
    <div class="text-center mb-[60px]">
      <h2 class="text-4xl text-dark dark:text-light mb-4 relative inline-block">
        Nuestros Servicios<div
          class="absolute bottom-[-10px] left-1/2 -translate-x-1/2 w-[200px] h-[4px] bg-primary rounded-md"
        >
        </div>
      </h2><p class="text-gray dark:text-light-gray max-w-3xl mt-5 mx-auto">
        Ofrecemos soluciones personalizadas para optimizar tus procesos
        empresariales.
      </p>
    </div>
    <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-8">
      {services.map(service => (
        <ServiceCard 
          icon={service.icon} 
          title={service.title} 
          description={service.description} 
        />
      ))}
    </div>
  </div>
</section>

<!-- Chat Example -->
